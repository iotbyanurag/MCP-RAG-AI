// Root endpoint
app.get('/', function (req, res) {
  res.json({ 
    message: 'DB Pod Ready - Enhanced Session Management Active',
    timestamp: new Date().toISOString(),
    sessionManagement: 'DB-Controlled'
  });
});

// =====================================
// ‚≠ê ENHANCED SESSION CLEANUP SCHEDULER ‚≠ê
// =====================================

const sessionDbAPI = require('./services/sessionDbApi');

console.log('üßπ Setting up enhanced session cleanup scheduler...');

// Function to perform session cleanup
function performSessionCleanup() {
  try {
    const now = Date.now(); // Keep in milliseconds for consistency
    const result = sessionDbAPI.deleteExpiredSessions(now);
    
    if (result && result.changes > 0) {
      console.log(`üßπ Session cleanup: removed ${result.changes} expired sessions at ${new Date().toLocaleTimeString()}`);
    } else {
      console.log(`üßπ Session cleanup: no expired sessions found at ${new Date().toLocaleTimeString()}`);
    }
    
    // Also clean up blacklisted tokens periodically
    if (verifyToken.tokenBlacklist) {
      console.log(`üóëÔ∏è Blacklist size: ${verifyToken.tokenBlacklist.size} tokens`);
    }
    
  } catch (error) {
    console.error("‚ùå Session cleanup error:", error.message);
  }
}

// Clean up expired sessions every 2 minutes (more frequent for 2-minute sessions)
const cleanupInterval = setInterval(performSessionCleanup, 2 * 60 * 1000);

console.log('üïí Session cleanup scheduler started (runs every 2 minutes)');

// Startup cleanup
console.log('üöÄ Performing startup session cleanup...');
performSessionCleanup();

// =====================================
// ‚≠ê ENHANCED ERROR HANDLING ‚≠ê
// =====================================

// Global error handler for session-related errors
app.use((err, req, res, next) => {
  console.error(`‚ùå Global error handler: ${err.message}`);
  console.error(`‚ùå Request: ${req.method} ${req.originalUrl}`);
  
  // Session-related errors
  if (err.message.includes('session') || err.message.includes('token')) {
    return res.status(401).json({
      error: 'Session error',
      message: 'Session expired. Please login again.',
      shouldLogout: true,
      timestamp: new Date().toISOString()
    });
  }
  
  // Other errors
  res.status(500).json({
    error: 'Internal server error',
    message: 'An unexpected error occurred',
    timestamp: new Date().toISOString()
  });
});

// =====================================
// ‚≠ê START SERVER ‚≠ê
// =====================================

var server = app.listen(17003, function () {
  var host = server.address().address;
  var port = server.address().port;
  
  console.log("üöÄ DB Pod listening at http://%s:%s", host, port);
  console.log("üîê Enhanced DB-controlled session management is active");
  console.log("üõ°Ô∏è All routes protected except login endpoints");
  console.log("‚è∞ 2-minute hard session expiry enforced");
  console.log("üßπ Automatic session cleanup every 2 minutes");
});

// =====================================
// ‚≠ê GRACEFUL SHUTDOWN ‚≠ê
// =====================================

function gracefulShutdown(signal) {
  console.log(`üõë ${signal} received, shutting down gracefully...`);
  
  // Clear cleanup interval
  if (cleanupInterval) {
    clearInterval(cleanupInterval);
    console.log('üßπ Session cleanup scheduler stopped');
  }
  
  // Perform final cleanup
  console.log('üßπ Performing final session cleanup...');
  performSessionCleanup();
  
  // Close server
  server.close(() => {
    console.log('üì¥ DB Pod HTTP server closed');
    process.exit(0);
  });
  
  // Force exit after 10 seconds
  setTimeout(() => {
    console.log('‚ö†Ô∏è Forced shutdown after timeout');
    process.exit(1);
  }, 10000);
}

process.on('SIGTERM', () => gracefulShutdown('SIGTERM'));
process.on('SIGINT', () => gracefulShutdown('SIGINT'));

// Handle uncaught exceptions
process.on('uncaughtException', (err) => {
  console.error('‚ùå Uncaught Exception:', err);
  gracefulShutdown('UNCAUGHT_EXCEPTION');
});

process.on('unhandledRejection', (reason, promise) => {
  console.error('‚ùå Unhandled Rejection at:', promise, 'reason:', reason);
  gracefulShutdown('UNHANDLED_REJECTION');
});

console.log('‚úÖ DB Pod initialization complete with enhanced session management');





api.js at backend pod 

const router = require('express').Router();
const express = require('express');
const cors = require('cors');
const axios = require('axios');
const configHost = require('../config');
const encryptDecrypt = require("../shared/encryptDecrypt");


const corsOptions = {
  origin: '*',
  credentials: true,
  optionSuccessStatus: 200
};


router.use(cors(corsOptions));
router.use(express.json());
router.use(express.urlencoded({ extended: true }));


// =====================================
// ‚≠ê ENHANCED LOGIN WITH COOKIE FORWARDING ‚≠ê
// =====================================


// Replace your Backend Pod login route with this bulletproof version


router.post('/users/login/', async function(req, res) {
  try {
    console.log('üîÑ Backend: Login request started');
   
    // Step 1: Decrypt user credentials
    const formData = req.body;
    console.log('üì• Backend: Form data keys:', Object.keys(formData));
   
    const userFirstName = formData.firstName;
    const paramsFirstName = encryptDecrypt.decryptData(userFirstName);
    const password = encryptDecrypt.decryptData(formData.password);
   
    console.log('üîê Backend: Decrypted user:', paramsFirstName);


    // Step 2: Forward to DB pod for authentication
    console.log('üîÑ Backend: Calling DB pod...');
    const dbResponse = await axios.post(`${configHost.destinationAddr}/api/login`, {
      paramsFirstName,
      password
    });


    const dbPasswordData = dbResponse.data;
    console.log('üì• Backend: DB response status:', dbResponse.status);
    console.log('üì• Backend: DB findUser:', dbPasswordData.findUser);


    // Step 3: Check if authentication failed
    if (!dbPasswordData.findUser) {
      console.log('‚ùå Backend: Authentication failed at DB level');
      return res.status(401).send('Authentication failed');
    }


    console.log('‚úÖ Backend: DB authentication successful');


    // Step 4: Get user role information (with error handling)
    let getOperator;
    try {
      console.log('üîÑ Backend: Getting user role...');
      const getOperatorRes = await axios.get(`${configHost.destinationAddr}/api/${paramsFirstName}`);
      getOperator = getOperatorRes.data;
      console.log('üì• Backend: User role:', getOperator.role);
    } catch (roleError) {
      console.error('‚ùå Backend: Failed to get user role:', roleError.message);
      // Use default role from DB response
      getOperator = {
        role: dbPasswordData.userRole || 'user',
        name: paramsFirstName,
        operatorName: null,
        profileImage: null
      };
    }


    // Step 5: Determine user permissions (simplified)
    const userRole = getOperator.role?.toLowerCase() || 'user';
    const isAdmin = userRole === 'admin';
    const isEngineer = userRole === 'engineer';
    const isOperator = userRole === 'operator';
    const isReadOnly = !isAdmin && !isEngineer && !isOperator;


    console.log('üë§ Backend: User permissions determined:', { isAdmin, isEngineer, isOperator, isReadOnly });


    // Step 6: Reset login attempts (with error handling)
    try {
      await axios.post(`${configHost.destinationAddr}/api/updateTimeAttempts`, {
        attempts: 0,
        currentTime: 0,
        paramsFirstName
      });
      console.log('üîÑ Backend: Login attempts reset');
    } catch (resetError) {
      console.warn('‚ö†Ô∏è Backend: Failed to reset login attempts:', resetError.message);
      // Continue anyway
    }


    // Step 7: Set authentication cookies
    if (dbPasswordData.token) {
      const maxAge = dbPasswordData.timeoutToken || 120000;
     
      console.log(`üç™ Backend: Setting cookies for role: ${userRole}`);
     
      // Set role-specific cookie
      res.cookie(`token_${userRole}`, dbPasswordData.token, {
        httpOnly: true,
        secure: false,
        sameSite: 'Strict',
        maxAge: maxAge,
        path: '/'
      });
     
      // Set generic cookie for fallback
      res.cookie('token', dbPasswordData.token, {
        httpOnly: true,
        secure: false,
        sameSite: 'Strict',
        maxAge: maxAge,
        path: '/'
      });
     
      console.log(`‚úÖ Backend: Cookies set successfully`);
    } else {
      console.error('‚ùå Backend: No token in DB response!');
      return res.status(500).send('Token generation failed');
    }


    // Step 8: Prepare response data (with error handling for encryption)
    let responseData;
    try {
      responseData = {
        isEngineer,
        isAdmin,
        isOperator,
        isReadOnly,
        userFirstName,
        loginName: encryptDecrypt.encryptData(getOperator.name || paramsFirstName),
        roleName: encryptDecrypt.encryptData(getOperator.role || userRole),
        operatorName: (getOperator.operatorName != null)
          ? encryptDecrypt.encryptData(getOperator.operatorName) : '',
        isPasswordChanges: false,
        profileImage: getOperator.profileImage || null,
        isClusterInstalled: dbPasswordData.isClusterInstalled || false,
        cellCount: dbPasswordData.cellCount || 0,
        firstLogin: dbPasswordData.userData?.firstLogin || 0,
        timeoutToken: dbPasswordData.timeoutToken,
        userRole: userRole
      };
     
      console.log('üì§ Backend: Response data prepared');
    } catch (encryptError) {
      console.error('‚ùå Backend: Encryption error:', encryptError.message);
      return res.status(500).send('Response preparation failed');
    }


    // Step 9: Send successful response
    console.log('‚úÖ Backend: Sending successful login response');
    return res.status(200).json(responseData);


  } catch (error) {
    console.error('‚ùå Backend: Critical login error:', error.message);
    console.error('‚ùå Backend: Error stack:', error.stack);
   
    // Return simple string error (not object) to avoid React rendering issues
    return res.status(500).send('Login failed due to server error');
  }
});


// Also add a simple logout route
router.post('/users/logout/', async function(req, res) {
  try {
    console.log('üö™ Backend: Logout request received');


    // Clear all possible cookies
    const cookiesToClear = ['token', 'token_admin', 'token_engineer', 'token_operator'];
   
    cookiesToClear.forEach(cookieName => {
      res.clearCookie(cookieName, {
        httpOnly: true,
        secure: false,
        sameSite: 'Strict',
        path: '/'
      });
    });


    console.log('üßπ Backend: Cleared all authentication cookies');
    return res.status(200).send('Logout successful');


  } catch (error) {
    console.error('‚ùå Backend logout error:', error.message);
    return res.status(500).send('Logout failed');
  }
});




// Generic route forwarding (for other API calls)
router.all('*', async function(req, res) {
  try {
    console.log(`üîÑ Backend: Forwarding ${req.method} ${req.originalUrl} to DB pod`);
   
    const response = await axios({
      method: req.method,
      url: `${configHost.destinationAddr}${req.originalUrl}`,
      data: req.body,
      headers: {
        'Content-Type': 'application/json',
        // ALWAYS forward cookies TO DB pod
        ...(req.headers.cookie && { 'Cookie': req.headers.cookie })
      },
      validateStatus: () => true
    });


    console.log(`üì• DB pod response: ${response.status} for ${req.originalUrl}`);


    // ALWAYS forward Set-Cookie headers FROM DB pod
    if (response.headers['set-cookie']) {
      response.headers['set-cookie'].forEach(cookieHeader => {
        res.setHeader('Set-Cookie', cookieHeader);
      });
    }


    res.status(response.status).json(response.data);


  } catch (error) {
    console.error(`‚ùå API forwarding error for ${req.originalUrl}:`, error.message);
    res.status(500).json({
      error: 'Request forwarding failed',
      message: error.message
    });
  }
});


// Basic routes
router.get('/', function(req, res) {
  res.json({
    message: 'Backend Pod API Ready',
    timestamp: new Date().toISOString(),
    cookieForwarding: 'Active'
  });
});


router.get('/route1', function(req, res) {
  res.json({ message: 'Route 1 handled by backend pod' });
});


router.get('/route2', function(req, res) {
  res.json({ message: 'Route 2 handled by backend pod' });
});


router.get('/route3', function(req, res) {
  res.json({ message: 'Route 3 handled by backend pod' });
});


module.exports = router;


verifytoken.js at db pod 

const crypto = require('crypto');
const jose = require('node-jose');
const config = require('../config');
const sessionDbAPI = require('../services/sessionDbApi');


// Store blacklisted tokens in memory
const tokenBlacklist = new Set();


// ‚≠ê 2-MINUTE SESSION CONFIGURATION ‚≠ê
const SESSION_TIMEOUT = 2 * 60 * 1000; // 2 minutes (120,000ms)


async function getJweKey(secretSource) {
  const secretKey = crypto.createHash('sha256').update(secretSource).digest();
  const base64url = secretKey.toString('base64')
    .replace(/\+/g, '-')
    .replace(/\//g, '_')
    .replace(/=+$/, '');
  const keyStore = jose.JWK.createKeyStore();
  return await keyStore.add({
    kty: 'oct',
    k: base64url,
    alg: 'A256GCM',
    use: 'enc'
  });
}


async function createJweToken(payload, key) {
  const token = await jose.JWE.createEncrypt({ format: 'compact' }, key)
    .update(JSON.stringify(payload))
    .final();
  return token;
}


async function decryptJweToken(token, key) {
  const result = await jose.JWE.createDecrypt(key).decrypt(token);
  return JSON.parse(result.plaintext.toString());
}


async function verifyToken(req, res, next) {
  const startTime = Date.now();
  const logs = [];
  const log = (msg) => {
    const timestamp = new Date().toLocaleTimeString();
    const logMsg = `[${timestamp}] ${msg}`;
    console.log(logMsg);
    logs.push(logMsg);
  };


  try {
    log(`üõ°Ô∏è DB SESSION CHECK: ${req.method} ${req.originalUrl}`);


    // Get token from cookies (role-specific or generic)
    let token = null;
    let tokenSource = 'none';
   
    // Check for role-specific tokens first
    const roles = ['admin', 'engineer', 'operator'];
    for (const role of roles) {
      const roleToken = req.cookies[`token_${role}`];
      if (roleToken) {
        token = roleToken;
        tokenSource = `token_${role}`;
        break;
      }
    }
   
    // Fallback to generic token
    if (!token) {
      token = req.cookies.token;
      tokenSource = 'token';
    }
   
    // Also check Authorization header
    if (!token && req.headers.authorization?.startsWith('Bearer ')) {
      token = req.headers.authorization.split(' ')[1];
      tokenSource = 'bearer_header';
    }


    if (!token) {
      log('‚ùå No token provided in cookies or headers');
      return res.status(401).json({
        error: 'No token provided',
        message: 'Session expired. Please login again.',
        shouldLogout: true,
        timestamp: new Date().toISOString(),
        logs
      });
    }


    log(`üîç Token found in: ${tokenSource}`);


    // Check blacklist
    if (tokenBlacklist.has(token)) {
      log(`üö´ Token is blacklisted`);
      return res.status(401).json({
        error: 'Token is blacklisted',
        message: 'Session expired. Please login again.',
        shouldLogout: true,
        timestamp: new Date().toISOString(),
        logs
      });
    }


    // ‚≠ê PRIMARY CHECK: Database session lookup ‚≠ê
    const session = sessionDbAPI.getSessionByToken(token);
    if (!session) {
      log('‚ùå Session not found in database');
      return res.status(401).json({
        error: 'Session not found',
        message: 'Session expired. Please login again.',
        shouldLogout: true,
        timestamp: new Date().toISOString(),
        logs
      });
    }


    log(`üîç Session found in DB: expires at ${new Date(session.expiresAt).toLocaleTimeString()}`);


    const now = Date.now();
    const timeRemaining = session.expiresAt - now;
    const timeRemainingSeconds = Math.floor(timeRemaining / 1000);


    // ‚≠ê HARD EXPIRY CHECK - No grace period for 2-minute sessions ‚≠ê
    if (timeRemaining <= 0) {
      log(`‚è∞ Session expired ${Math.abs(timeRemainingSeconds)} seconds ago`);
     
      // Immediately deactivate expired session
      try {
        sessionDbAPI.deactivateSessionByToken(token);
        log('üóëÔ∏è Expired session deactivated');
      } catch (dbError) {
        log(`‚ö†Ô∏è Error deactivating session: ${dbError.message}`);
      }
     
      return res.status(401).json({
        error: 'Session expired',
        message: 'Session expired. Please login again.',
        shouldLogout: true,
        timeRemaining: 0,
        expiredBy: Math.abs(timeRemainingSeconds),
        timestamp: new Date().toISOString(),
        logs
      });
    }


    // ‚≠ê DECRYPT AND VERIFY TOKEN PAYLOAD ‚≠ê
    let payload;
    try {
      const jweKey = await getJweKey(config.jwtSecretKey);
      payload = await decryptJweToken(token, jweKey);
      log(`üßæ Token decrypted successfully for user: ${payload.username || payload.userName}`);
    } catch (decryptError) {
      log(`‚ùå Token decryption failed: ${decryptError.message}`);
      return res.status(401).json({
        error: 'Invalid token format',
        message: 'Session expired. Please login again.',
        shouldLogout: true,
        timestamp: new Date().toISOString(),
        logs
      });
    }


    // ‚≠ê UPDATE LAST ACTIVITY (Track but don't extend) ‚≠ê
    try {
      sessionDbAPI.updateSessionActivity(token);
      log('üì± Session activity updated');
    } catch (activityError) {
      log(`‚ö†Ô∏è Could not update activity: ${activityError.message}`);
    }


    // ‚≠ê NO AUTO-REFRESH FOR 2-MINUTE SESSIONS ‚≠ê
    // Hard 2-minute expiry - no sliding window, no token refresh
    log(`‚úÖ Session valid: ${timeRemainingSeconds}s remaining (hard expiry)`);


    // Attach user info to request
    req.user = payload;
    req.session = {
      ...session,
      timeRemaining: timeRemainingSeconds
    };


    const processingTime = Date.now() - startTime;
    log(`‚úÖ DB session validation completed in ${processingTime}ms`);


    return next();


  } catch (err) {
    const processingTime = Date.now() - startTime;
    log(`‚ùå Token verification failed after ${processingTime}ms: ${err.message}`);
   
    return res.status(401).json({
      error: 'Session validation failed',
      message: 'Session expired. Please login again.',
      shouldLogout: true,
      timestamp: new Date().toISOString(),
      logs
    });
  }
}


// ‚≠ê BLACKLIST MANAGEMENT ‚≠ê
verifyToken.tokenBlacklist = tokenBlacklist;


verifyToken.blacklistToken = function (token) {
  if (token) {
    tokenBlacklist.add(token);
    console.log(`üóëÔ∏è Token blacklisted (size: ${tokenBlacklist.size})`);
   
    // Remove from blacklist after 5 minutes (tokens expire in 2 minutes anyway)
    setTimeout(() => {
      tokenBlacklist.delete(token);
      console.log(`‚ôªÔ∏è Token removed from blacklist (size: ${tokenBlacklist.size})`);
    }, 5 * 60 * 1000);
  }
};


// ‚≠ê SESSION CLEANUP FUNCTION ‚≠ê
verifyToken.deleteExpiredSessions = function(currentTime) {
  try {
    return sessionDbAPI.deleteExpiredSessions(currentTime);
  } catch (error) {
    console.error('‚ùå Session cleanup error:', error.message);
    return { changes: 0 };
  }
};


module.exports = verifyToken;
