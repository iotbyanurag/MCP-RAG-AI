index.js at backend pod 

const express = require('express');
const fs = require('fs');
const path = require('path');
const cookieParser = require('cookie-parser');
const cors = require('cors');
const axios = require('axios');


const app = express();
const port = 17002;


// üç™ GLOBAL COOKIE FORWARDING SOLUTION - ONE PLACE FIX
let currentRequest = null;


// Middleware to capture current request
// Add this debug middleware to your Backend Pod index.js (after CORS, before other routes)


app.use((req, res, next) => {
  // Only log non-static requests
  if (!req.originalUrl.includes('/static/') && !req.originalUrl.includes('.js') && !req.originalUrl.includes('.css')) {
    console.log(`üîÑ Backend: ${req.method} ${req.originalUrl}`);
    console.log(`üîÑ Backend: Request cookies:`, Object.keys(req.cookies || {}));
   
    // Log specific tokens if they exist
    if (req.cookies.token_admin) {
      console.log(`üîÑ Backend: token_admin present, length: ${req.cookies.token_admin.length}`);
    }
    if (req.cookies.token) {
      console.log(`üîÑ Backend: token present, length: ${req.cookies.token.length}`);
    }
  }
  next();
});


// Also add enhanced forwarding for all API routes
app.use('/api', async (req, res, next) => {
  // Skip if it's the login route (handled separately)
  if (req.path.includes('/users/login')) {
    return next();
  }
 
  try {
    console.log(`üîÑ Backend: Forwarding ${req.method} /api${req.path} to DB pod`);
    console.log(`üîÑ Backend: Forwarding cookies:`, Object.keys(req.cookies || {}));
   
    const response = await axios({
      method: req.method,
      url: `${configHost.destinationAddr}/api${req.path}`,
      data: req.body,
      headers: {
        'Content-Type': 'application/json',
        // CRITICAL: Forward ALL cookies to DB pod
        ...(req.headers.cookie && { 'Cookie': req.headers.cookie })
      },
      validateStatus: () => true
    });


    console.log(`üì• Backend: DB pod response ${response.status} for /api${req.path}`);
   
    // Forward any Set-Cookie headers from DB pod
    if (response.headers['set-cookie']) {
      response.headers['set-cookie'].forEach(cookieHeader => {
        res.setHeader('Set-Cookie', cookieHeader);
      });
    }


    return res.status(response.status).json(response.data);


  } catch (error) {
    console.error(`‚ùå Backend: API forwarding error for /api${req.path}:`, error.message);
    return res.status(500).json({
      error: 'Request forwarding failed',
      message: error.message
    });
  }
});


// Enhanced forwarding for all other routes
app.use('*', async (req, res, next) => {
  // Skip if already handled by other middleware
  if (res.headersSent) {
    return;
  }
 
  try {
    console.log(`üîÑ Backend: Forwarding ${req.method} ${req.originalUrl} to DB pod`);
   
    const response = await axios({
      method: req.method,
      url: `${configHost.destinationAddr}${req.originalUrl}`,
      data: req.body,
      headers: {
        'Content-Type': 'application/json',
        // CRITICAL: Forward ALL cookies to DB pod
        ...(req.headers.cookie && { 'Cookie': req.headers.cookie })
      },
      validateStatus: () => true
    });


    console.log(`üì• Backend: DB pod response ${response.status} for ${req.originalUrl}`);
   
    // Forward any Set-Cookie headers from DB pod
    if (response.headers['set-cookie']) {
      response.headers['set-cookie'].forEach(cookieHeader => {
        res.setHeader('Set-Cookie', cookieHeader);
      });
    }


    return res.status(response.status).json(response.data);


  } catch (error) {
    console.error(`‚ùå Backend: Forwarding error for ${req.originalUrl}:`, error.message);
    return res.status(500).json({
      error: 'Request forwarding failed',
      message: error.message
    });
  }
});


// Routers
const apiRouter = require('./routes/api');
const PTPConfigRouter = require('./routes/PTPConfigRouter');
const userRouter = require('./routes/userRouter');
